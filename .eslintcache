b_react/src/containers/Layout/CardItem.js",[],"/home/her0ftheday/Projects/web/web-lab_react/src/containers/Layout/CatalogLayout.js",["176","177","178","179","180"],"import React, { useState } from \"react\";\n\nimport { Container, Row } from \"reactstrap\";\nimport { Dropdown, DropdownButton, Form, Col, Spinner } from \"react-bootstrap\";\nimport RangeSlider from \"react-bootstrap-range-slider\";\nimport CardsLayout from \"./CardsLayout\";\nimport getItems from \"../../webapi/CarApiUtil\";\nimport EPaceImage from \"../../assets/items/e-pace.jpg\";\nimport FPaceImage from \"../../assets/items/f-pace.jpg\";\nimport XJImage from \"../../assets/items/xj.jpg\";\nimport XFImage from \"../../assets/items/xf.jpg\";\n\n// var items = [\n//   {\n//     img: EPaceImage,\n//     title: \"Jaguar E-Pace 2020\",\n//     year: \"2020\",\n//     price: 80000,\n//     key: 1,\n//   },\n\n//   {\n//     img: FPaceImage,\n//     title: \"Jaguar F-Pace 2020 SUV \",\n//     year: \"2020\",\n//     price: 10000,\n//     key: 2,\n//   },\n\n//   {\n//     img: XJImage,\n//     title: \"Jaguar XJ Coupe 2016\",\n//     year: \"2016\",\n//     price: 45646,\n//     key: 3,\n//   },\n\n//   {\n//     img: XFImage,\n//     title: \"Jaguar XF Coupe 2012\",\n//     year: \"2012\",\n//     price: 87898,\n//     key: 4,\n//   },\n// ];\n\nconst CatalogLayout = () => {\n  const [loading, setLoading] = useState(false);\n  const items = getItems(setLoading);\n  const [cars, setCarsList] = useState(items);\n  const [searchTerm, setSearchTerm] = useState(\"\");\n\n  const handleChange = (event) => {\n    setSearchTerm(event.target.value);\n  };\n\n  const results = !searchTerm\n    ? cars\n    : cars.filter((car) =>\n        car.title.toLowerCase().includes(searchTerm.toLocaleLowerCase())\n      );\n\n  function sortByYearAsc() {\n    const sortedCars = [...cars].sort((a, b) => a.year - b.year);\n    setCarsList(sortedCars);\n  }\n\n  function sortByYearDesc() {\n    const sortedCars = [...cars].sort((a, b) => b.year - a.year);\n    setCarsList(sortedCars);\n  }\n\n  function sortByPriceAsc() {\n    const sortedCars = [...cars].sort((a, b) => a.price - b.price);\n    setCarsList(sortedCars);\n  }\n\n  function sortByPriceDesc() {\n    const sortedCars = [...cars].sort((a, b) => b.price - a.price);\n    setCarsList(sortedCars);\n  }\n\n  if (!loading) {\n    return (\n      <div className=\"pt-5 mt-5 mb-5 pb-5 d-flex justify-content-center\">\n        <Spinner\n          animation=\"border\"\n          role=\"status\"\n          className=\"justify-content-center\"\n        >\n          <span className=\"sr-only justify-content-center align-self-center\">\n            Loading...\n          </span>\n        </Spinner>\n      </div>\n    );\n  } else\n    return (\n      <div className=\"pt-5\">\n        <Container>\n          <Row className=\"pt-5 justify-content-between\">\n            <Form>\n              <Form.Row>\n                <Form.Group as={Col} controlId=\"formGridEmail\">\n                  <Form.Control\n                    placeholder=\"Search\"\n                    value={searchTerm}\n                    onChange={handleChange}\n                    onKeyPress={(e) => {\n                      e.key === \"Enter\" && e.preventDefault();\n                    }}\n                  />\n                </Form.Group>\n              </Form.Row>\n            </Form>\n            <DropdownButton variant=\"secondary\" title=\"Sort By\">\n              <Dropdown.Item eventKey=\"1\" active>\n                Popularity\n              </Dropdown.Item>\n              <Dropdown.Divider />\n              <Dropdown.Item eventKey=\"2\" onClick={sortByYearAsc}>\n                Year Asc\n              </Dropdown.Item>\n              <Dropdown.Item eventKey=\"3\" onClick={sortByYearDesc}>\n                Year Desc\n              </Dropdown.Item>\n              <Dropdown.Divider />\n              <Dropdown.Item eventKey=\"4\" onClick={sortByPriceAsc}>\n                Price Asc\n              </Dropdown.Item>\n              <Dropdown.Item eventKey=\"5\" onClick={sortByPriceDesc}>\n                Price Desc\n              </Dropdown.Item>\n            </DropdownButton>\n          </Row>\n        </Container>\n        <Container\n          className=\"pt-5 d-flex flex-wrap justify-content-around center\"\n          style={{\n            maxWidth: \"1300px\",\n          }}\n        >\n          <CardsLayout items={results} />\n        </Container>\n      </div>\n    );\n};\n\nexport default CatalogLayout;\n","/home/her0ftheday/Projects/web/web-lab_react/src/containers/Layout/CardsLayout.js",[],"/home/her0ftheday/Projects/web/web-lab_react/src/containers/Layout/ItemDetailsLayout.js",["181","182","183"],"import React, { useState } from \"react\";\nimport { Container, Row, Col, Media, CardImg, Card } from \"reactstrap\";\nimport {\n  Image,\n  Button,\n  DropdownButton,\n  Dropdown,\n  Toast,\n} from \"react-bootstrap\";\nimport { Link } from \"react-router-dom\";\nimport { actionCreators } from \"../../redux/actions\";\nimport { useDispatch } from \"react-redux\";\n\nconst ItemDetailsLayout = (props) => {\n  const dispatch = useDispatch();\n\n  const [showAdd2CartToast, setAdd2CartToast] = useState(false);\n\n  const toggleShowB = () => setAdd2CartToast(!showAdd2CartToast);\n\n  const handleAddToCart = (item) => {\n    dispatch(actionCreators.addToList(item));\n    toggleShowB();\n  };\n\n  const { title, price, img, year, id } = props.location.state;\n  return (\n    <div className=\"pt-5 mb-5\">\n      <Row className=\"justify-content-end mr-5 mt-5\">\n        <Toast onClose={toggleShowB} show={showAdd2CartToast}>\n          <Toast.Header ></Toast.Header>\n          <Toast.Body>Added to Cart!</Toast.Body>\n        </Toast>\n      </Row>\n      <Container className=\"mt-5 pt-5\">\n        <Row className=\" align-content-around mb-5\">\n          <Container style={{ maxHeight: \"500px\", maxWidth: \"450px\" }}>\n            <Image src={img} fluid rounded />\n          </Container>\n          <Col>\n            <h3>{title}</h3>\n            <p>\n              Porro labore quisquam adipisci porro dolor numquam est. Labore\n              dolorem labore aliquam neque. Ut quiquia sit dolorem modi eius\n              quiquia. Quiquia ipsum consectetur sed dolorem ut. Consectetur\n              ipsum eius eius sit eius dolorem.\n            </p>\n            <hr />\n            <Row className=\"justify-content-between mb-3\">\n              <h5 className=\"ml-3\">Price: {numberWithCommas(price)}£</h5>\n              <DropdownButton\n                id=\"dropdown-basic-button\"\n                title=\"Available colours\"\n                variant=\"secondary\"\n              >\n                <Dropdown.Item href=\"#\">Silver</Dropdown.Item>\n                <Dropdown.Item href=\"#\">Dark</Dropdown.Item>\n                <Dropdown.Item href=\"#\">Bright red</Dropdown.Item>\n              </DropdownButton>\n            </Row>\n            <Row className=\"justify-content-end\">\n              <Row className=\"align-self-around\">\n                <Link exact to=\"/browse\">\n                  <Button variant=\"outline-dark\" className=\"mr-1\">\n                    Go Back\n                  </Button>\n                </Link>\n                <Button\n                  variant=\"danger\"\n                  onClick={() =>\n                    handleAddToCart({ title, price, img, year, id })\n                  }\n                >\n                  Add To Cart\n                </Button>\n              </Row>\n            </Row>\n          </Col>\n        </Row>\n      </Container>\n    </div>\n  );\n};\n\nfunction numberWithCommas(x) {\n  return x.toString().replace(/\\B(?=(\\d{3})+(?!\\d))/g, \",\");\n}\n\nexport default ItemDetailsLayout;\n","/home/her0ftheday/Projects/web/web-lab_react/src/webapi/CarApiUtil.js",[],"/home/her0ftheday/Projects/web/web-lab_react/src/redux/actions.js",[],"/home/her0ftheday/Projects/web/web-lab_react/src/redux/reducers.js",["184"],"const initialState = {\n    items: []\n  };\n  \n  const updateObjectInArray = (array, action) => {\n    return array.map((item, index) => {\n      if (index !== action.payload) {\n        return item;\n      }\n  \n      return {\n        ...item,\n        inBasket: true\n      };\n    });\n  };\n  \n  const removeItem = (array, action) => {\n    return array.filter((item, index) => index !== action.payload);\n  };\n  \n  export default (state = initialState, action) => {\n    switch (action.type) {\n      case \"ADD_TO_LIST\":\n        return {\n          ...state,\n          items: [\n            ...state.items,\n            {            \n              value: action.payload,\n              inBasket: false\n            }\n          ]\n        };\n      case \"ADD_TO_BASKET\":\n        return {\n          ...state,\n          items: updateObjectInArray(state.items, action)\n        };\n      case \"REMOVE_ITEM\":\n        return {\n          ...state,\n          items: removeItem(state.items, action)\n        };\n      case \"CLEAR_ITEMS\": {\n        return {\n          items: []\n        };\n      }\n      default:\n        return state;\n    }\n  };\n  ","/home/her0ftheday/Projects/web/web-lab_react/src/containers/Layout/BasketItem.js",["185","186","187"],"/home/her0ftheday/Projects/web/web-lab_react/src/components/Checkout.js",["188","189"],"/home/her0ftheday/Projects/web/web-lab_react/src/components/SuccessCart.js",["190","191","192"],"import React from \"react\";\nimport { Container, Row, Col, Button } from \"react-bootstrap\";\nimport { TiTick } from \"react-icons/ti\";\nimport { Link } from \"react-router-dom\";\nimport SuccessSVG from \"../assets/success.svg\";\n\nconst SuccessCart = () => {\n  return (\n    <div>\n      <Container className=\"pt-5 mt-5 pb-5 text-center\">\n        <Row className=\"justify-content-center\">\n          {/* <TiTick color=\"green\" size=\"10vw\" /> */}\n          <Container className=\"mb-3\"\n            style={{\n              maxWidth: \"200px\",\n              maxHeight: \"250px\",\n            }}\n          >\n            <img src={SuccessSVG} />\n          </Container>\n        </Row>\n        <h3\n          style={{\n            fontWeight: \"bold\",\n          }}\n        >\n          Success!\n        </h3>\n        <p>\n          Your order was sent to processing!\n          <br />\n          Check your email box for further information.\n        </p>\n        <Link exact to=\"/browse\">\n          <Button variant=\"outline-dark\">Go back to Browse</Button>\n        </Link>\n      </Container>\n    </div>\n  );\n};\n\nexport default SuccessCart;\n","/home/her0ftheday/Projects/web/web-lab_react/src/components/Signup.js",["193"],"/home/her0ftheday/Projects/web/web-lab_react/src/components/Login.js",["194","195"],"/home/her0ftheday/Projects/web/web-lab_react/src/authentication/AuthenticationContext.js",[],"/home/her0ftheday/Projects/web/web-lab_react/src/history/History.js",[],"/home/her0ftheday/Projects/web/web-lab_react/src/containers/App/PublicApp.js",["196","197","198","199","200","201"],{"ruleId":"202","severity":1,"message":"203","line":1,"column":17,"nodeType":"204","messageId":"205","endLine":1,"endColumn":30},{"ruleId":"202","severity":1,"message":"206","line":3,"column":3,"nodeType":"204","messageId":"205","endLine":3,"endColumn":23},{"ruleId":"202","severity":1,"message":"207","line":4,"column":3,"nodeType":"204","messageId":"205","endLine":4,"endColumn":6},{"ruleId":"202","severity":1,"message":"208","line":5,"column":3,"nodeType":"204","messageId":"205","endLine":5,"endColumn":12},{"ruleId":"202","severity":1,"message":"209","line":6,"column":3,"nodeType":"204","messageId":"205","endLine":6,"endColumn":12},{"ruleId":"202","severity":1,"message":"210","line":7,"column":3,"nodeType":"204","messageId":"205","endLine":7,"endColumn":9},{"ruleId":"211","replacedBy":"212"},{"ruleId":"213","replacedBy":"214"},{"ruleId